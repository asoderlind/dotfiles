#Application

super + w
	"$BROWSER"

super + d
	dmenu_run

super + shift + t
   thunar

{XF86MonBrightnessUp,XF86MonBrightnessDown}
	xbacklight -{inc,dec} 5 && kill -46 $(pidof dwmblocks)

{XF86AudioLowerVolume, XF86AudioRaiseVolume}
	pamixer --allow-boost -{d,i} 5 && kill -44 $(pidof dwmblocks)

{XF86Display}
  /home/axel/.local/bin/toggle_pad

shift + Print
    maim -su | xclip -selection c -t image/png

ctrl + Print
    maim -i \"$(xdotool getactivewindow)\" | xclip -selection c -t image/png
